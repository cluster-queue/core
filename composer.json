{
    "name": "cluster-queue/core",
    "description": "Setup a configuration to setup a server cluster. Eg. with DRDB, heartbeat for DB/ WWW Servers; You may look for the dummy package to use this in the background",
    "keywords": ["php", "library", "web", "shell", "DTO", "DAO"],
    "homepage": "https://github.com/cluster-queue/core",
    "authors": [
        {
            "name": "Florian Blasel",
            "email": "flobee.code@gmail.com"
        }
    ],
    "type": "library",
    "repositories": [{
        "type":"package",
        "package": {
          "name": "mumsys/library",
          "version":"master",
          "source": {
              "url": "https://github.com/mumsys/library.git",
              "type": "git",
              "reference":"unstable"
            }
        }
    }],

    "bin": [
        "bin/php-cluster-queue"
    ],

    "autoload": {
        "classmap": [
            "src/",
            "vendor/mumsys/library/src"
        ]
    },

    "require": {
        "php": "7.3.* | 7.4.* | 8.0.*",
        "mumsys/library": "master"
    },

    "require-dev": {

    },

    "config": {
        "sort-packages": true
    },

    "license": "GPL-3.0",

    "minimum-stability": "dev"
}
