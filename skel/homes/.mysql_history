; some commands
SHOW STATUS;
SHOW PROCESSLIST;
SELECT User,Host FROM mysql.user;
CREATE USER 'root'@'10.0.0.%' IDENTIFIED BY 'password';
GRANT ALL PRIVILEGES ON *.* TO 'root'@'10.0.0.%' WITH GRANT OPTION;
FLUSH PRIVILEGES;
;
REVOKE ALL PRIVILEGES, GRANT OPTION FROM 'root'@'10.0.0.%';
DROP USER 'root'@'10.0.0.%';
;
CREATE USER 'pma'@localhost IDENTIFIED BY 'password';
GRANT SELECT, INSERT, DELETE, UPDATE, ALTER ON `phpmyadmin`.* TO 'pma'@localhost;
